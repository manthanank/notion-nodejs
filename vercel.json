{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/public/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "rewrites": [
      {
        "source": "/public/(.*)",
        "destination": "/public/$1"
      }
    ]
  }